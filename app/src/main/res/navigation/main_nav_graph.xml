<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/signinFragmentFragment">

    <fragment
        android:id="@+id/signinFragmentFragment"
        android:name="com.example.chatapplication.view.authentication.SignInFragment"
        android:label="fragment_signin_fragment"
        tools:layout="@layout/fragment_signin" >
        <action
            android:id="@+id/action_signinFragmentFragment_to_showAllChatFragment"
            app:destination="@id/showAllChatFragment" />
    </fragment>
    <fragment
        android:id="@+id/showAllChatFragment"
        android:name="com.example.chatapplication.view.chat.ShowAllChatFragment"
        android:label="fragment_show_all_chat"
        tools:layout="@layout/fragment_show_all_chat" />
</navigation>